{
  "name": "gallery-x",
  "version": "1.0.0",
  "description": "The 'X' makes it sound cool~",
  "scripts": {
    "postinstall": "cd server && npm install && cd ../static && npm install",
    "dev-server": "cd server && cross-env NODE_ENV=development npm start",
    "dev-static": "cd static && cross-env NODE_ENV=development npm start",
    "build-server": "cd server && npm run build",
    "build-static": "cd static && cross-env NODE_ENV=development npm run build",
    "tslint": "tslint -c tslint.json -p . --fix",
    "precommit": "lint-staged",
    "test": "npm test"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VenDream/Gallery-X.git"
  },
  "keywords": [
    "Pixiv",
    "Gallery"
  ],
  "author": "VenDream",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VenDream/Gallery-X/issues"
  },
  "homepage": "https://github.com/VenDream/Gallery-X#readme",
  "devDependencies": {
    "@types/camelcase-keys": "^4.0.0",
    "@types/node": "^10.9.4",
    "@types/qs": "^6.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0"
  },
  "dependencies": {
    "camelcase-keys": "^4.2.0",
    "cross-env": "^5.2.0",
    "qs": "^6.5.2",
    "ts-node": "^6.2.0",
    "typescript": "^3.0.3"
  }
}
